.dataList
  width 750px
  background white
  .DataListwrapper
    width 100%
    padding-left 30px
    box-sizing border-box
    .item
      width 100%
      height 91px
      display inline-block
      line-height 84px
      position relative
      &::after
        position absolute
        content ""
        width 100%
        height 2px
        left 0
        bottom 0px
        background #eee
      span
        display inline-block
        width 100%
        height 42px
        line-height 102px
        font-size 28px
        color #333 