.SellWell
  width 100%
  padding-bottom 20px
  margin-bottom 40px
  .warper
    width 100%
    .header
      width 100%
      height 90px
      padding 0 30px
      line-height 90px
      font-size 32px
      color #333
      position relative 
      display flex
      justify-content space-between
      box-sizing border-box
      a
        display flex
        font-size 28px
        .icon
          background-image: url(//yanxuan-static.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/icon-normal/index-titleArrow-193a1a730c.png)
          background-size 100% 100%
          width 20px
          height 20px
          margin-top 35px
          margin-left 5px
    .main
      width 100%
      height 553px
      padding 0 30px
      box-sizing border-box
      .wellList
        display flex
        flex-wrap wrap
        width 100%
        .wellItem1
          background-size 100% 100% 
          width 335px
          height 200px
          margin-right 10px
          margin-bottom 10px 
          background #F9F3E4
          position relative
          .text
            font-size 28px
            position absolute
            left 25px
            top 65px
            &::before
              content ''
              position absolute
              left 0
              bottom -20px
              width 48px
              height 4px
              background #333333
          .img
            width 200px
            height 200px
            float right 
            img 
              width 200px
              height 200px   
        .wellItem2
          background-size 100% 100% 
          width 335px
          height 200px
          margin-right 10px
          margin-bottom 10px 
          background #EBEFF6
          position relative
          .text
            font-size 28px
            position absolute
            left 25px
            top 65px
            &::before
              content ''
              position absolute
              left 0
              bottom -20px
              width 48px
              height 4px
              background #333
          .img
            width 200px
            height 200px
            float right 
            img 
              width 200px
              height 200px  
      .wells
        width 100%
        display flex
        flex-wrap wrap
        .item
          width 23.5%
          height 180px
          background #F5F5F5
          margin 0 10px 10px 0
          box-sizing border-box
          .text
            padding-top 10px 
            font-size 24px
            color #333
            text-align center
          .img
            width 120px
            height 120px
            margin 0 auto
            img
              width 120px
              height 120px