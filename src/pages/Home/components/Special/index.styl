.Special
  width 100%
  padding-bottom 20px
  .warper
    width 100%
    .header
      width 100%
      height 90px
      padding 0 30px
      line-height 90px
      font-size 32px
      color #333
      position relative 
      display flex
      justify-content space-between
      box-sizing border-box
      a
        display flex
        font-size 28px
        .icon
          background-image: url(//yanxuan-static.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/icon-normal/index-titleArrow-193a1a730c.png)
          background-size 100% 100%
          width 20px
          height 20px
          margin-top 35px
          margin-left 5px
    .main
      width 100%
      height 553px
      padding 0 30px
      box-sizing border-box
      .specialList
        display flex
        flex-wrap wrap
        width 100%
        height 100%
